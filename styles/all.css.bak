/* ---------------------------------------------------------
	1. Embedded fonts styles
	2. General reset
	3. Common site styles
	4. Custom form styles
	5. Styles for responsive breakpoints
	6. Styles for ratina
	---------------------------------------------------------*/
/* ---------------------------------------------------------
	1. Embedded fonts styles
	---------------------------------------------------------*/
@font-face {
	font-family: 'helveticaneue-mediumregular';
	src:url('../fonts/helveticaneue-medium-webfont-webfont.eot');
	src:url('../fonts/helveticaneue-medium-webfont-webfont.eot?#iefix') format('embedded-opentype'),
		url('../fonts/helveticaneue-medium-webfont-webfont.woff') format('woff'),
		url('../fonts/helveticaneue-medium-webfont-webfont.ttf') format('truetype'),
		url('../fonts/helveticaneue-medium-webfont-webfont.svg#helveticaneue-mediumregular') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'helveticaneue-mediumitalicRg';
	src:url('../fonts/helveticaneue-mediumitalic-webfont-webfont.eot');
	src:url('helveticaneue-mediumitalic-webfont-webfont.eot?#iefix') format('embedded-opentype'),
		url('../fonts/helveticaneue-mediumitalic-webfont-webfont.woff') format('woff'),
		url('../fonts/helveticaneue-mediumitalic-webfont-webfont.ttf') format('truetype'),
		url('../fonts/helveticaneue-mediumitalic-webfont-webfont.svg#helveticaneue-mediumitalicRg') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'helveticaneuebold';
	src:url('../fonts/helveticaneue_bold-webfont-webfont.eot');
	src:url('../fonts/helveticaneue_bold-webfont-webfont.eot?#iefix') format('embedded-opentype'),
		url('../fonts/helveticaneue_bold-webfont-webfont.woff') format('woff'),
		url('../fonts/helveticaneue_bold-webfont-webfont.ttf') format('truetype'),
		url('../fonts/helveticaneue_bold-webfont-webfont.svg#helveticaneuebold') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'helveticaneue-bolditalicRg';
	src:url('../fonts/helveticaneue-bolditalic-webfont-webfont.eot');
	src:url('../fonts/helveticaneue-bolditalic-webfont-webfont.eot?#iefix') format('embedded-opentype'),
		url('../fonts/helveticaneue-bolditalic-webfont-webfont.woff') format('woff'),
		url('../fonts/helveticaneue-bolditalic-webfont-webfont.ttf') format('truetype'),
		url('../fonts/helveticaneue-bolditalic-webfont-webfont.svg#helveticaneue-bolditalicRg') format('svg');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'helveticaneue-blackregular';
	src:url('../fonts/helveticaneue-black-webfont-webfont.eot');
	src:url('../fonts/helveticaneue-black-webfont-webfont.eot?#iefix') format('embedded-opentype'),
		url('../fonts/helveticaneue-black-webfont-webfont.woff') format('woff'),
		url('../fonts/helveticaneue-black-webfont-webfont.ttf') format('truetype'),
		url('../fonts/helveticaneue-black-webfont-webfont.svg#helveticaneue-blackregular') format('svg');
	font-weight: normal;
	font-style: normal;
}
/* ---------------------------------------------------------
	2. General reset
	---------------------------------------------------------*/
html { height: 100%; }
body {
	min-width: 1003px;
	height: 100%;
	margin: 0;
	color: #282626;
	font: 18px/21px 'helveticaneuebold', Arial, sans-serif;
	background: #fff;
	-webkit-text-size-adjust: 100%;
	-ms-text-size-adjust: none;
}
img { border: 0; }
/* set styles for default page links */
a {
	text-decoration: none;
	color: #282626;
}
a:hover { text-decoration: underline; }
a:active { background-color: transparent; }
/* set default styles in all browsers */
input, textarea, select {
	font: 100% 'helveticaneue-mediumregular', Arial, sans-serif;
	vertical-align: middle;
	color: #033339;
}
form, fieldset {
	margin: 0;
	padding: 0;
	border-style: none;
}
header, footer, article, section, hgroup, nav, figure { display: block; }
input[type=text],
input[type=checkbox],
input[type=password],
input[type=file],
input[type=submit],
input[type="email"],
button[type="button"],
textarea {
	-webkit-appearance: none;
	-webkit-border-radius: 0 !important;
	-ms-border-radius: 0 !important;
	border-radius: 0 !important;
	border: 0 !important;
}
input[type=submit]:hover { cursor: pointer; }
/* placeholder input styles */
::-webkit-input-placeholder { /* WebKit browsers */
	color: #033339;
}
:-moz-placeholder { /* Mozilla Firefox 4 to 18 */
	color: #033339;
}
::-moz-placeholder { /* Mozilla Firefox 19+ */
	color: #033339;
	opacity: 1;
}
:-ms-input-placeholder { /* Internet Explorer 10+ */
	color: #033339;
}
.alignleft { float: left; }
.alignright { float: right; }
/* ---------------------------------------------------------
	3. Ð¡ommon site styles
	---------------------------------------------------------*/
/* main container styles */
#wrapper {
	overflow: hidden;
	position:relative;
}
.bg-blue { background: #05b2c7; }
/* main informative part styles */
#main {
	max-width: 1024px;
	margin: 0 auto;
	height: 100%;
	padding: 55px 0 0;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.bg-blue #main { padding: 65px 0 60px; }
.coach-details-page #main { padding: 55px 0 105px;}
.live-chat-page #main { padding: 55px 0 0;}
.messages-page #mian{ padding: 55px 0 57px;}
/* page logo styles */
#main .logo {
	width: 503px;
	height: 245px;
	display: block;
	margin: 0 auto 33px;
	text-indent: -9999px;
	overflow: hidden;
	position: relative;
	left: -14px;
	background: url(../images/sprite.png) no-repeat;
}
.logo {
	text-indent: -9999px;
	overflow: hidden;
}
.logo a {
	display: block;
	height: 100%;
}
/* login form styles */
.login-form {
	max-width: 370px;
	margin: 0 auto;
	text-align: center;
	overflow:hidden;
}
.login-form input[type="text"],
.login-form input[type="email"],
.login-form input[type="password"] {
	width: 100%;
	height: 59px;
	margin: 0 0 1px;
	color: #033339;
	outline: none;
	box-shadow: none;
	text-align: center;
	font-size: 23px;
	line-height: 29px;
	padding: 15px;
	border-radius: 3px !important;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.login-form input[type="submit"] {
	width: 100%;
	height: 59px;
	margin: 14px 0 13px;
	color: #fff;
	text-transform: capitalize;
	border-radius: 3px !important;
	font-size: 23px;
	line-height: 29px;
	padding: 15px 15px;
	background: #ff9914;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.login-form a.forgot {
	display: inline-block;
	vertical-align: top;
	color: #b6fe09;
	text-transform: capitalize;
	font: 23px/29px 'helveticaneue-mediumregular', Arial, sans-serif;
}
/* footer styles */
#footer {
	position: fixed;
	bottom: 0;
	left: 0;
	right: 0;
	height: 40px;background: #05b2c7;
}
/* for signup styles */
.signup {
	max-width: 1024px;
	margin: 0 auto;
	padding: 10px 0;
	text-align: center;
	color: #fefefe;
	font: 23px/29px 'helveticaneue-mediumregular', Arial, sans-serif;
	background: #05b2c7;
}
.signup p { margin: 0; }
.signup p a { color: #b6fe09; }
/* welcome area styles */
.welcome-area {
	padding: 39px 0 0;
	text-align: center;
}
.welcome-area a.btn {
	min-width: 280px;
	padding: 5px 10px;
	display: inline-block;
	vertical-align: top;
	color: #b6fe09;
	margin: 0 0 20px;
	background: #0394a6;
	border-radius: 3px;
	font: 28px/39px 'helveticaneue-mediumregular', Arial, sans-serif;
}
.welcome-area ul {
	list-style: none;
	padding: 59px 0 0;
	margin: 0;
}
.welcome-area li {
	width: 140px;
	display: inline-block;
	vertical-align: top;
	margin: 0 30px 30px 34px;
}
.welcome-area li a .img-holder span.hover { display: none; }
.welcome-area li a:hover .img-holder span.normal { display: none; }
.welcome-area li a:hover .img-holder span.hover { display: block; }
.welcome-area .img-holder {
	width: 122px;
	margin: 0 auto 9px;
}
.welcome-area img {
	display: block;
	width: 100%;
	height: auto;
}
.welcome-area .title {
	display: inline-block;
	vertical-align: top;
	text-transform: uppercase;
	font: 17px/20px 'helveticaneuebold', Arial, sans-serif;
}
.welcome-area li a {
	color: #048797;
	display: block;
}
.welcome-area li a:hover { color: #fff; }
/* header styles */
#header {
	height: 55px;
	background: #05b2c7;
	margin: 0 auto;
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	z-index:9999;
}
.header-holder {
	max-width: 1024px;
	height: 55px;
	margin: 0 auto;
}
#header .btn-back {
	width: 54px;
	height: 55px;
	float: left;
	padding: 18px 0 0 21px;
	border-right: 1px solid #048e9f;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
#header .btn-back a {
	width: 10px;
	height: 17px;
	display: block;
	text-indent: -9999px;
	overflow: hidden;
	background: url(../images/sprite.png) no-repeat -28px -450px;
}
#header h1 {
	float: left;
	font-size: 22px;
	line-height: 35px;
	font-weight: normal;
	padding: 10px 22px;
	color: #fff;
	margin: 0;
	border-left: 1px solid #76d5e0;
}
/* nav holder styles */
.nav-holder { float: right; }
.nav-holder a.filter {
	width: 39px;
	height: 24px;
	float: left;
	font-size: 0;
	line-height: 0;
	margin: 15px 36px 0 0;
	position: relative;
	background: url(../images/sprite.png) no-repeat -59px -418px;
}
.nav-holder a.filter:after {
	width: 0;
	height: 0;
	content: "";
	position: absolute;
	top: -3px;
	right: 1px;
	border-style: solid;
	border-width: 5px 4px 0 4px;
	border-color: #fff transparent transparent transparent;
}
.nav-holder a.inbox {
	width: 31px;
	height: 20px;
	float: left;
	margin: 18px 30px 0 0;
	position: relative;
	background: url(../images/sprite.png) no-repeat -136px -420px;
}
.nav-holder a.inbox .counter {
	width: 19px;
	height: 19px;
	text-align: center;
	border-radius: 100%;
	position: absolute;
	top: -9px;
	right: -6px;
	color: #b6fe09;
	font: 10px/19px 'helveticaneue-blackregular', Arial, sans-serif;
	background: #026976;
}
/* main navigation styles */
#nav {
	width: 55px;
	height: 55px;
	position: relative;
	float: left;
	padding: 18px;
	font: 20px/33px 'helveticaneue-mediumregular', Arial, sans-serif;
	background: url(../images/border.png) repeat-y;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
#nav .nav-opener {
	width: 22px;
	height: 18px;
	float: left;
	position: relative;
	text-indent: -9999px;
	overflow: hidden;
	background: url(../images/sprite.png) no-repeat -57px -450px;
}
.overlay {
	position:fixed;
	top:0;
	left:0;
	right:0;
	bottom:0;
	z-index:10;
	background:#000;
}
#nav .slide-block {
	position: absolute;
	top: 55px;
	right: 0;
	width: 324px;
	background: #fff;
	z-index: 999;
	overflow-y:auto;
	overflow-x:hidden;
}
#nav .nav-slide {
	position:relative;
	padding: 63px 27px 10px 28px;
	width:100%;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
#nav .nav-slide:after {
	content:'';
	display:block;
	clear:both;
}
#nav .close {
	width: 21px;
	height: 20px;
	position: absolute;
	top: 23px;
	left: 27px;
	text-indent: -9999px;
	overflow: hidden;
	background: url(../images/sprite.png) no-repeat -208px -421px;
	z-index:50;
}
#nav a.email {
	display: block;
	margin: 0 0 25px;
	text-align: center;
	color: #fff;
	font-size: 19px;
	line-height: 25px;
	padding: 5px;
	background: #cdcccc;
}
#nav .nav-slide > ul {
	list-style: none;
	padding: 0;
	margin: 0;
	border-top: 1px solid #cdcccc;
}
#nav .nav-slide > ul > li { border-bottom: 1px solid #cdcccc; }
#nav .nav-slide > ul > li > a {
	display: block;
	position: relative;
	padding: 10px 40px 10px 10px;
	color: #05b2c7;
}
#nav .nav-slide .has-dropdown > a:after {
	width: 33px;
	content: "";
	position: absolute;
	top: 0;
	right: 0;
	bottom: 0;
	z-index: 99;
	background: #cdcccc url(../images/arrow-dropdown.png) no-repeat 50% 50%;
}
.submenu {
	width: 100%;
	position: absolute;
	top: 0;
	left: 100%;
	padding: 95px 30px 0;
	font: 15px/20px 'helveticaneuebold', Arial, sans-serif;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.submenu .back-menu {
	position: absolute;
	top: 23px;
	right: 17px;
	font-size: 17px;
	padding: 0 0 0 22px;
	color: #05b2c7;
}
.submenu .back-menu:after {
	width: 11px;
	height: 19px;
	content: "";
	position: absolute;
	top: 1px;
	left: 0;
	background: url(../images/sprite.png) no-repeat -240px -422px;
}
.submenu ul {
	width: 140px;
	list-style: none;
	padding: 0 12px 0 0;
	margin: 0 auto;
}
.submenu li { margin: 0 0 29px; }
.submenu .img-holder {
	width: 110px;
	margin: 0 auto 4px;
}
.submenu .img-holder img {
	display: block;
	width: 100%;
	height: auto;
}
.submenu li a:hover .img-holder span.hover,
.submenu .img-holder span.normal { display: block; }
.submenu li a:hover .img-holder span.normal,
.submenu .img-holder span.hover { display: none; }
.submenu .title {
	display: block;
	text-transform: uppercase;
	text-align: center;
	font-weight: normal;
}
.submenu li a {
	color: #b7b7b7;
	display:block;
}
.submenu li a:hover {
	color: #05b2c7;
	text-decoration: none;
}
/* filter header styled */
.filter-header {
	margin: 0 -9999px;
	padding: 8px 9999px 11px;
	background: #eeecec;
	overflow: hidden;
}
.filter-header a.btn-filter {
	font-size: 18px;
	line-height: 22px;
	float: left;
	padding: 0 23px 0 0;
	position: relative;
	color: #026976;
	margin: 0 18px;
}
.filter-header a.btn-filter:after {
	width: 0;
	height: 0;
	content: "";
	position: absolute;
	top: 7px;
	right: 0;
	border-style: solid;
	border-width: 11px 8.5px 0 8.5px;
	border-color: #026876 transparent transparent transparent;
}
/* coachs styled */
.coachs article {
	position: relative;
	padding: 18px 280px 14px 31px;
}
.coachs .box { overflow: hidden; }
.coachs .img-holder,
.coachs .alignleft {
	width: 93px;
	border: 5px solid #d8dcd8;
	margin: 0 28px 0 0;
}
.coachs .img-holder img,
.coachs .alignleft img {
	width: 100%;
	display: block;
	height: auto;
}
.coachs .text { overflow: hidden; }
.coachs .fav {
	width: 35px;
	text-align: center;
	float: right;
	padding: 11px 8px 10px 20px;
	text-transform: uppercase;
}
.coachs .fav a {
	width: 35px;
	height: 35px;
	display: block;
	margin: 0 auto 2px;
	text-indent: -9999px;
	overflow: hidden;
	border-radius: 100%;
	background: #c1c2c1 url(../images/sprite.png) no-repeat 8px -413px;
}
.coachs .fav.active a { background-color: #f7d94b; }
.coachs .fav strong {
	display: block;
	font-size: 16px;
	color: #c1c2c1;
}
.coachs .title {
	font-size: 23px;
	line-height: 23px;
	display: block;
	font-weight: normal;
	margin: 0 0 4px;
	text-transform: capitalize;
}
.coachs .title a { color: #026976; }
.coachs .star-rating { margin: 0 0 10px;}
.coachs .star-rating img {
	display: block;
	width: 102px;
	height: auto;
}
.coachs .status {
	text-transform: uppercase;
	font-size: 19px;
	line-height: 23px;
	margin: 0 0 10px;
	color: #b0b1b0;
}
.coachs .status span.online { color: #8bc403; }
.coachs .status span.online,
.coachs .status.active span.offline { display: none; }
.coachs .status.active span.online,
.coachs .status span.offline { display: block; }
.coachs p { margin: 0; }
.coachs a.more {
	width: 95px;
	border-radius: 3px;
	background: #ff9914;
	color: #fff;
	position: absolute;
	top: 50%;
	right: 69px;
	padding: 8px 40px 9px 17px;
	margin: -20px 0 0;
	font-size: 15px;
	line-height: 17px;
}
.coachs a.more:after {
	width: 8px;
	height: 13px;
	content: "";
	position: absolute;
	top: 50%;
	right: 17px;
	margin: -6px 0 0;
	background: url(../images/sprite.png) 0 -452px;
}
/* info styled */
#footer .info {
	font-size: 20px;
	line-height: 30px;
	text-align: center;
	background: #eeecec;
}
#footer .info-holder {
	max-width: 1024px;
	margin: 0 auto;
	padding: 5px 0;
}
#footer .info p { margin: 0; }
#footer .info p strong {
	color: #026976;
	font-weight: normal;
}
#footer .info p a { color: #ff8814; }
/* contact options styled */
.contact {
	text-align: center;
	background: #37c1af;
}
.contact-holder {
	max-width: 1024px;
	margin: 0 auto;
}
.contact ul {
	width: 100%;
	height: 105px;
	list-style: none;
	padding: 0;
	margin: 0;
	display: table;
}
.contact li:first-child {
	width: 32%;
	border: none;
}
.contact li {
	width: 30.56%;
	height: 100%;
	display: table-cell;
	vertical-align: top;
	border-left: 2px solid #fff;
}
.contact li.chat { width: 36.32%; }
.contact a {
	display: block;
	height: 100%;
	color: #fff;
	padding: 11px 5px 5px;
	box-sizing: border-box;
	-moz-box-sizing: border-box;
	-webkit-box-sizing: border-box;
}
.contact .img-holder {
	width: 56px;
	display: block;
	margin: 0 auto 8px;
}
.contact img {
	display: block;
	width: 100%;
	height: auto;
}
.contact .title {
	display: block;
	margin: 0 0 5px;
	font: 15px/15px 'helveticaneue-blackregular', Arial, sans-serif;
}
.contact .rate {
	display: block;
	font: 15px/15px 'helveticaneue-mediumregular', Arial, sans-serif;
}
/* coach details styles */
.coach-details {
	padding: 16px 42px 40px 31px;
}
.coach-details .alignleft {
	width: 93px;
	border: 5px solid #d8dcd8;
	margin: 0 28px 0 0;
}
.coach-details .personal-info {
	overflow: hidden;
	font-size: 18px;
	margin: 0;
	padding: 14px 0 17px;
}
.coach-details .personal-info dt {
	color: #026976;
	float: left;
	margin: 0 5px 0 0;
}
.coach-details .personal-info dd { margin: 0 0 17px}
.coach-details .personal-info ul {
	list-style: none;
	padding: 0;
	margin: 0
}
.coach-details .personal-info li {
	float: left;
	margin: 0 5px 0 0;
}
.coach-details .personal-info li:after { content: ","; }
.coach-details .personal-info li.last:after { display: none; }
/* office hours styles */
.office-hours {
	max-width: 382px;
	margin: 0 0 34px;
}
.office-hours .heading {
	display: block;
	text-align: center;
	font-size: 18px;
	font-weight: normal;
	margin: 0 0 12px;
	text-transform: uppercase;
}
.office-hours table {
	width: 100%;
	border-collapse: collapse;
	font-size:16px;
	line-height: 20px;
	color: #fff;
	text-transform: capitalize;
}
.office-hours td:first-child { width: 56%; }
.office-hours td {
	padding: 4px 14px;
	background: #c1c2c1;
	border-bottom: 1px solid #fff;
}
.office-hours tr.even td { background: #a6a6a6; }
/* expert in styles */
.expert-in {
	overflow: hidden;
	padding: 0 2px 0 0;
}
.expert-in .heading {
	float: left;
	font-size: 18px;
	font-weight: normal;
	margin: 8px 13px 0 0;
}
.expert-in ul {
	list-style: none;
	padding: 0;
	margin: 0;
	overflow: hidden;
}
.expert-in li {
	display: inline-block;
	vertical-align: middle;
	margin: 0 0 0 26px;
}
/* live chat sstyles */
.live-chat:after {
	content: "";
	clear: both;
	display: block;
}
.live-chat { padding: 0 0 0 275px; }
