<!-- main container of all the page elements -->
<div id="wrapper" class="bg-blue">
	<!-- contain main informative part of the site -->
	<div id="main">
		<!-- page logo -->
		<strong class="logo"><a href="#">FANTASY COACHING STAFF</a></strong>
		<!-- login form -->
		<form action="#/welcome/" class="login-form">
			<fieldset>
				<input type="email" placeholder="Email">
				<input type="password" placeholder="Password">
				<input type="submit" value="Login" onClick='document.getElementById("loginoverlay").style.display="block";'>
				<a href="#" class="forgot">Forgot Username/Password?</a>
			</fieldset>
		</form>
		<!-- footer of the page -->
		<div id="footer1">
			<!-- for signup -->
			<div class="signup">
				<p>Don’t have an FCS account? <a href="#/signup/">Sign-Up</a></p>
			</div>
		</div>
	</div>
	
</div>
